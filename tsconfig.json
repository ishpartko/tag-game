{
  "compilerOptions": {
      "target": "es6",
      "allowJs": true,
      "lib": ["dom", "dom.iterable", "esnext"],
      "skipLibCheck": true,
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "src/*"
        ]
      }
  },
  "exclude": [
    "node_modules",
    "ignored",
  ]
}
